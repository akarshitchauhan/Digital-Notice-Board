{
  "manifest_version": 3,
  "name": "Time Display Extension",
  "description": "Shows the current time and interacts with a server.",
  "version": "1.0",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": ["activeTab", "storage", "http://localhost:4000/*"],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "128": "icon.png"
  }
}
